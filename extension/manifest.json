{
  "name": "Mortgage calculator",
  "version": "1.0",
  "author": "majestic905@gmail.com",
  "manifest_version": 2,
  "permissions": ["storage"],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:; font-src https://fonts.gstatic.com"
}